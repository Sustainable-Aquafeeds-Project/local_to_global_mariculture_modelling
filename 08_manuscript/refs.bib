@article{magni_animal-sediment_2009,
	title = {Animal-sediment relationships: {Evaluating} the ‘{Pearson}–{Rosenberg} paradigm’ in {Mediterranean} coastal lagoons},
	volume = {58},
	issn = {0025-326X},
	shorttitle = {Animal-sediment relationships},
	url = {https://www.sciencedirect.com/science/article/pii/S0025326X08005651},
	doi = {10.1016/j.marpolbul.2008.12.009},
	abstract = {We investigated the applicability of the Pearson–Rosenberg (P–R) conceptual model describing a generalized pattern of response of benthic communities in relation to organic enrichment to Mediterranean Sea coastal lagoons. Consistent with P–R model predictions, benthic diversity and abundance showed two different peaks at low ({\textgreater}2.5–5mgg−1) and high ({\textgreater}25–30mgg−1) total organic carbon (TOC) ranges, respectively. We identified TOC thresholds indicating that risks of reduced benthic diversity should be relatively low at TOC values{\textless}about 10mgg−1, high at TOC values{\textgreater}about 28mgg−1, and intermediate at values in-between. Predictive ability within these ranges was high based on results of re-sampling simulation. While not a direct measure of causality, it is anticipated that these TOC thresholds should serve as a general screening-level indicator for evaluating the likelihood of reduced sediment quality and associated bioeffects in such eutrophic systems of the Mediterranean Sea.},
	number = {4},
	urldate = {2025-03-05},
	journal = {Marine Pollution Bulletin},
	author = {Magni, P. and Tagliapietra, D. and Lardicci, C. and Balthis, L. and Castelli, A. and Como, S. and Frangipane, G. and Giordani, G. and Hyland, J. and Maltagliati, F. and Pessa, G. and Rismondo, A. and Tataranni, M. and Tomassetti, P. and Viaroli, P.},
	month = apr,
	year = {2009},
	keywords = {Mediterranean Sea, Eutrophication, Benthic diversity, Coastal lagoons, Ecological indicators, Macrobenthos},
	pages = {478--486},
}

