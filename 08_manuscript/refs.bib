	year = {2020},
	doi = {10.1016/j.aquaculture.2020.735474},
	note = {MAG ID: 3024761681},
	pages = {735474},
	file = {Björn Kok et al. 2020 - Fish as feed Using economic allocation to quantif.pdf:C\:\\Users\\treimer\\OneDrive - University of Tasmania\\Zotero\\Björn Kok et al. 2020 - Fish as feed Using economic allocation to quantif.pdf:application/pdf},
