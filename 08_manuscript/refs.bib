	pages = {298--303},
	file = {Halley E. Froehlich et al. 2018 - Avoiding the ecological limits of forage fish for.pdf:C\:\\Users\\treimer\\OneDrive - University of Tasmania\\Zotero\\Halley E. Froehlich et al. 2018 - Avoiding the ecological limits of forage fish for.pdf:application/pdf},
