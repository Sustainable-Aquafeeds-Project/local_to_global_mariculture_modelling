	keywords = {Temperature, Growth, Atlantic salmon post-smolts, Feed conversion efficiency, Stomach evacuation rate},
	pages = {36--42},
	file = {Handeland et al. 2008 - The effect of temperature and fish size on growth,.pdf:C\:\\Users\\treimer\\OneDrive - University of Tasmania\\Zotero\\Handeland et al. 2008 - The effect of temperature and fish size on growth,.pdf:application/pdf},
