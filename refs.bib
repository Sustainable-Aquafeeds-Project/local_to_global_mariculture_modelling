	month = may,
	year = {2020},
	doi = {10.1038/s43016-020-0078-x},
	note = {MAG ID: 3026119784},
	pages = {301--308},
	file = {Cottrell et al. 2020 - Global adoption of novel aquaculture feeds could s.pdf:C\:\\Users\\treimer\\OneDrive - University of Tasmania\\Zotero\\Cottrell et al. 2020 - Global adoption of novel aquaculture feeds could s.pdf:application/pdf},
